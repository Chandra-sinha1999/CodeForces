#include<iostream>
using namespace std;
int main(void)
{
    int n;
    cin>>n;
    int res=0;
    while(n--)
    {
        int a,b,c;
        int count =0;
        cin>>a>>b>>c;
        if(a==1)
            count+=1;
        if(b==1)
            count+=1;
        if(c==1)
            count+=1;
        if(count>=2)
            res+=1;
    }
    cout<<res<<endl;
}
