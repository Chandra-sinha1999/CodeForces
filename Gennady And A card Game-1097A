#include<iostream>
using namespace std;
#include<cstring>
int main(void)
{
    string a,b,c,d,e,f;
    cin>>a;
    cin>>b>>c>>d>>e>>f;
    int x=0;
    if(b[0]==a[0]||b[0]==a[1]||b[1]==a[0]||b[1]==a[1])
    {
        x=1;
    }
    else if(c[0]==a[0]||c[0]==a[1]||c[1]==a[0]||c[1]==a[1])
    {
        x=1;
    }
    else if(d[0]==a[0]||d[0]==a[1]||d[1]==a[0]||d[1]==a[1])
    {
        x=1;
    }
    else if(e[0]==a[0]||e[0]==a[1]||e[1]==a[0]||e[1]==a[1])
    {
        x=1;
    }
    else if(f[0]==a[0]||f[0]==a[1]||f[1]==a[0]||f[1]==a[1])
    {
        x=1;
    }
    /*for(int i=0;i<b.length();i++)
    {
        if(b[i]==a[0]||b[i]==a[1])
        {
            x=1;
            break;
        }
    }
    if(x==1)
    {
        cout<<"YES"<<endl;
    }
    else
        cout<<"NO"<<endl;
}
